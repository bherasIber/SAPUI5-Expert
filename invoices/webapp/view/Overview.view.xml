<mvc:View controllerName="logaligroup.invoices.controller.App"
        xmlns="sap.m"
        xmlns:mvc="sap.ui.core.mvc" 
        displayBlock="true">
    <Page id="page" title="{i18n>appTitle}">
        <headerContent>
                <Button id="helloDialogHeader" 
                        icon="sap-icon://action" 
                        press=".onOpenDialogHeader" 
                        class="sapUiSmallMarginEnd" />   
        </headerContent>
        <content>
                <mvc:XMLView id="viewHelloPanel" viewName="logaligroup.invoices.view.HelloPanel"/>
                <mvc:XMLView id="viewInvoicesList" viewName="logaligroup.invoices.view.InvoicesList"/>
        </content>
    </Page>

</mvc:View>
